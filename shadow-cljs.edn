{:deps true
 :deps-aliases [:cljs]
 :dev-http {3000 "resources/public"}
 :nrepl {:port 0}
 :builds
 {:app {:target :browser
        :output-dir "resources/public/js"
        :asset-path "/js"
        :modules {:main {:entries [loicb.me.ui.core]}}
        :release {:module-hash-names 8}
        :devtools {:after-load loicb.me.ui.core/render!}}}}
